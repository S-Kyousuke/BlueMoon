<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />
	<inherits name='com.badlogic.gdx.ai' />
	<inherits name='com.google.common.collect.Collect' />

    <inherits name='th.skyousuke.libgdx.bluemoon.GdxDefinition' />
    
    <collapse-all-properties />
    
	<add-linker name="xsiframe"/>	
	<set-configuration-property name="devModeRedirectEnabled" value="true"/>
	<set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>
    <extend-configuration-property name="gdx.reflect.include" value="th.skyousuke.libgdx.bluemoon.game.ui.InventoryWindow.InventoryWindowStyle" />
    <extend-configuration-property name="gdx.reflect.include" value="th.skyousuke.libgdx.bluemoon.game.ui.Window.WindowStyle" />
</module>
